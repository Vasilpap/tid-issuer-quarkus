# --- Database (via Compose; host-mapped port) ---
QUARKUS_DATASOURCE_JDBC_URL=jdbc:postgresql://localhost:5432/quarkusdb
QUARKUS_DATASOURCE_USERNAME=quarkus
QUARKUS_DATASOURCE_PASSWORD=quarkus

# --- Keycloak (via Compose; host-mapped port) ---
# Realm and client must match realm-export.json
QUARKUS_OIDC_AUTH_SERVER_URL=http://localhost:8180/realms/tid-issuer
QUARKUS_OIDC_CLIENT_ID=quarkus-api
QUARKUS_OIDC_CREDENTIALS_SECRET=

# --- MinIO (object storage) ---
MINIO_URL=http://localhost:9000
MINIO_ACCESS_KEY=replace-me-minio-access-key
MINIO_SECRET_KEY=replace-me-minio-secret-key
MINIO_BUCKET=articles-of-association

# --- HTTP / CORS (Vue dev server) ---
QUARKUS_HTTP_PORT=8080
CORS_ALLOWED_ORIGINS=http://localhost:5173
